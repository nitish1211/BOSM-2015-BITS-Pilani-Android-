<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="neo.bosm2015.bits.com.bosm2015.Developers">
<RelativeLayout
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/big_card">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="25sp"
        android:textColor="@color/material_blue_grey_800"
        android:textStyle="bold|italic"
        android:text="Developers "
        android:id="@+id/textView6"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="21dp" />


    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/NitishPic"
        android:padding="1dp"
        android:background="@drawable/round_button"
        android:src="@drawable/profile"
        android:layout_below="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:layout_marginTop="30dp" />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/JaiPic"
        android:padding="1dp"
        android:background="@drawable/round_button"
        android:src="@drawable/profile"
        android:layout_below="@+id/NitishPic"
        android:layout_alignStart="@+id/textView6"
        android:layout_marginTop="30dp" />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/AshitPic"
        android:padding="1dp"
        android:background="@drawable/round_button"
        android:src="@drawable/profile"
        android:layout_below="@+id/JaiPic"
        android:layout_alignStart="@+id/textView6"
        android:layout_marginTop="30dp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:textColor="@color/card_black"
        android:textSize="18sp"
        android:text="Nitish Kundala"
        android:id="@+id/nitish"
        android:layout_alignTop="@+id/NitishPic"
        android:layout_toEndOf="@+id/textView6"
        android:layout_marginTop="24dp" />
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:textColor="@color/card_black"
        android:textSize="18sp"
        android:text="Jai Agarwal"
        android:id="@+id/jai"
        android:layout_alignTop="@+id/JaiPic"
        android:layout_toEndOf="@+id/textView6"
        android:layout_marginTop="24dp" />
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:textColor="@color/card_black"
        android:textSize="18sp"
        android:text="Ashit Shrivastava"
        android:id="@+id/ashit"
        android:layout_alignTop="@+id/AshitPic"
        android:layout_toEndOf="@+id/textView6"
        android:layout_marginTop="24dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       android:textSize="20sp"
        android:textStyle="bold"
        android:text="By DVM"
        android:id="@+id/textView7"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="26dp" />




</RelativeLayout>
</RelativeLayout>
